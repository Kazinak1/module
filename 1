#include <ctype.h>
#include <cs50.h>
#include <stdio.h>

int main(void)
{
    int b = 0;
    int n = get_int("Количество чисел: ");
    int a[n];
    for (int i = 0; i < n; i++)
    {
        a[i] = get_int("Введите число %i: ", (i+1));
        if ((a[i] <= 0) || (a[i] > 100))
        {
            b++;
        }
    }
    printf("%i", a[1]);
    if (!(b == 0))
    {
        printf("Число должно быть от 1 до 100");
    }
    else
    {
        for (int i = n; i < 0; i--)
        {
            printf("%i ", a[i]);
        }
    }
}
