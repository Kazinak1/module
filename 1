#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n, b = 0;
    cout << "Количество чисел: ";
    cin >> n;
    int a[n];
    for (int i = 0; i < n; i++) 
    {
        cin >> a[i];
        if ((a[i] <= 0) || (a[i] > 100))
        {
            b++;
        }
    }
    if (!(b == 0))
    {
        cout << Число должно быть от 1 до 100";
        return 1;
    }
    for (int i = n-1; i >= 0; i--)
    {
        cout << a[i] << " ";
    }
    return 0;
}
